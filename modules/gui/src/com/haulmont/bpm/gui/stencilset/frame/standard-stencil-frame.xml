<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        class="com.haulmont.bpm.gui.stencilset.frame.AbstractStencilFrame"
        focusComponent="fieldGroup.title"
        messagesPack="com.haulmont.bpm.gui.stencilset">
    <dsContext>
        <datasource id="stencilDs"
                    class="com.haulmont.bpm.entity.stencil.StandardStencil">
        </datasource>
    </dsContext>
    <layout expand="fieldGroup" spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="stencilDs"
                    editable="false">
            <column width="200px">
                <field id="title"/>
                <field id="stencilId"/>
            </column>
        </fieldGroup>
    </layout>
</window>
